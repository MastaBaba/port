
PX = (1/16)rem
gs = 10*PX

rect()
  width  arguments
  height arguments
nw()
  top  arguments
  left arguments
circle()
  rect arguments
  border-radius 50%

easy-click()
  &:after
    content ''
    position absolute
    rect 200%
    z-index 1
    nw -50%

c-deep-purple = #28143f
body
  background c-deep-purple
  font-size 16px
  margin  0
  padding 0
  user-select none
  overflow hidden
  font-family Open Sans, sans-serif
  min-width 768px
  // perspective 1000px
  // &::-webkit-scrollbar
  //   overflow -moz-scrollbars-none !important
  //   display none !important
  //   -ms-overflow-style none !important
  //   width 0 !important
   
// .element
//   border 1px solid hotpink
//   padding 40px
