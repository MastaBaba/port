.badge
  position fixed
  nw 0
  z-index 10
  color white
  
.wrapper
  position    absolute
  z-index     1
  top         0
  bottom      0
  left        0
  right       0
  overflow    hidden
    
.particles
  perspective 500px
  perspective-origin 50% 50%
  overflow visible
  position relative
  user-select none
  
  position absolute
  // overflow hidden
  z-index 1
  tap-highlight-color rgba(0,0,0,0)
  // width 2000px
  transform translateZ(0)
  user-select none
  text-size-adjust none
  // background #fff

particleSize = 150/2
contentWidth  = 540
contentHeight = 280
{el = '.particle'}
  rect 2*particleSize*PX
  position absolute
  nw 0
  text-align center
  display block
  // overflow hidden
  line-height (2*particleSize)*PX
  &.is-open
    z-index 20
    {el}__inner
      transform scale(15) translateZ(-1000px)
    {el}__content
      display block
  &__inner
    circle 100%
    position absolute
    nw 0
    transform translateZ(0)
  &__over-mobile
    display none
    .desktop &
      display block
    // background-image url('i/mojs-transparent.png')
    // background-size 300% auto
    // background-position 340px -10px

// svg
//   border 2px solid red !important

.content
  position      fixed
  width         contentWidth*PX
  height        contentHeight*PX
  display       block
  margin-left  -(contentWidth/2)*PX
  margin-top   -(contentHeight/2)*PX
  // border        1px solid white
  z-index       2
  nw            50%
  // opacity       0
  transform     translateX(-5000px)
  // display       none
  &__inner
    position absolute
    rect 100%
  &__dust-wrapper
    // border 2px solid white
    // position    absolute
    // top         220px
    // width       240px
    // height      60px
    // overflow    hidden
    // z-index     2
    
  &.is-show
    display     block

.project
  color white
  position relative
  rect 100%
  // border 1px solid white
  &__text, 
  &__img
    position absolute
    width 50%
    height 100%
  &__text
    left 57%
    margin-top 3*gs
    width 40%
    text-align left
    line-height 1.6
  h2
    text-transform uppercase
  // &__dust-wrap
  //   // border 2px solid white
  //   position    absolute
  //   top         220px
  //   width       240px
  //   height      60px
  //   overflow    hidden
  //   z-index     1
  &__shadow-wrap
    transform-origin 100% center
  &__shadow
    position absolute
    width 300px
    height 50px
    opacity 0
    top 204px
    right 8px
    transform rotateX(80deg)
    border-radius 26px
    background linear-gradient(to right, rgba(58,8,57,0) 0%,rgba(58,8,57,0.65) 100%)
    transform-origin 100% center
    .ie &
      display none
    // box-shadow 0 0 150px #333


