.by
  background    white
  position      absolute
  bottom        0
  // right         15.3*gs
  // right         0
  left          0
  height        28*PX
  width         110*PX
  z-index       40
  opacity       .5
  border-top-right-radius 12*PX
  &:hover
    opacity     1
  &__text
    color           c-deep-purple
    font-size       8.5*PX
    letter-spacing  .2*PX
    position        absolute
    top             9*PX
    &--1
      left 7*PX
    &--2
      left 70*PX
  &__logo
    background    #f1f1f1
    position      absolute
    top           7*PX
    circle        16*PX
    background url('') no-repeat center center
    background-size 100% 100%
    &--mojs
      left 49*PX
      background-image url('i/mojs.svg')
    &--legomushroom
      left 85*PX
      background-image url('i/legomushroom.svg')

.sprite
  &__frame
    position    absolute
    nw          0
    display     block
    transform   translateZ(0)
    rect        100%
  &__ellipse
    fill        none
    stroke      white
    
.dust
  position  absolute
  // z-index   10
  // z-index   1
  rect      200*PX
  height    50*PX
  &--1
    top     270*PX
    left    100*PX
  &--2,
  &--3,
  &--4
    top     227*PX
  &--2
    left   -40*PX
    opacity .9
  &--3
    left   -70*PX
    opacity .7
  &--4
    left   -77*PX
    opacity .7
  
  
blobSize = 100
.blob-circle
  position      absolute
  nw           -50%
  z-index       1
  margin-left  -(blobSize/2)*PX
  margin-top   -(blobSize/2)*PX
  rect          blobSize*PX

.blob-circle-wrap
  width   0px
  height  0px
  overflow hidden

.close
  position fixed
  circle 30*PX
  top  20%
  left -10%
  z-index -1
  // opacity 0
  transition opacity .25s ease-out
  display none
  &:hover
    cursor pointer
  &.is-show
    display block
    opacity 1
    left 75%
    z-index 10
